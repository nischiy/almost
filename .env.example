
# Minimal example for preflight (paper/testnet safe)
ENV=dev
EXCHANGE=BINANCE
SYMBOL=BTCUSDT
INTERVAL=1m
QUOTE_ASSET=USDT
ORDER_QTY_USD=10
RISK_MAX_TRADES_PER_DAY=5
PAPER_TRADING=1
TRADE_ENABLED=0
BINANCE_TESTNET=true

# Optional risk
RISK_MAX_LOSS_USD_DAY=100
RISK_MAX_DD_PCT_DAY=5
RISK_MAX_POS_USD=1000
RISK_MIN_EQUITY_USD=50

# Live creds (required only if TRADE_ENABLED=1 and not paper/testnet)
# BINANCE_API_KEY=
# BINANCE_API_SECRET=
